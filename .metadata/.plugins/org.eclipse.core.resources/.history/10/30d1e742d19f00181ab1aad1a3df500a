
public class Media {

	public static void main(String[] args) {
		/* Faça um programa que calcule a média dos parâmetros passados para o método main. Crie uma
		classe chamada Media para implementar esse programa. Dica: para converter strings para double
		utilize o método parseDouble()
		*/
		//String s = "10";
		//double d = Double.parseDouble(s);
		
		java.util.Arrays.sort(args);

		for(String arg : args){
			//System.out.println("Teste Ordenando Array Passando Argumentos: "+ arg);
			double d = Double.parseDouble(arg);
			
			d += d;
			System.out.println("total dos parametros: "+d);
		}

	}

}
